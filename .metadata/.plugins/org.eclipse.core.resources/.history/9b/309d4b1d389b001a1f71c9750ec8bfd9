package ¶¯Ì¬¹æ»®;

public class L70 {
	 public int climbStairs(int n) {
	        if(n==1) return 1;
	        int a = 1;
	        int b = 1;
	        int sum =0;
	        for(int i =1 ;i<n;i++){
	            sum = a +b ;
	             b = a ;
	             a = sum; 
	        }
	        return a ;

	    }
}
